import * from "materials/scripts/units_base.material"
import * from "materials/scripts/gui.material"

material units/ground/spawner_cosmic_morirot : 		units/ground/black_blood
{
	set_texture_alias albedo			materials/textures/units/ground/spawner_cosmic_morirot_albedo.dds
	set_texture_alias normal			materials/textures/units/ground/spawner_morirot_normal.dds
	set_texture_alias packed			materials/textures/units/ground/spawner_morirot_packed.dds
	set_texture_alias emissive			materials/textures/units/ground/spawner_cosmic_morirot_emissive.dds
	set_texture_alias gradient			materials/textures/gradient/cloudy_gradient.dds 174 5

	glow 15

	set_texture_alias albedo_2			materials/textures/units/ground/spawner_cosmic_morirot_damage_albedo.dds
	set_texture_alias packed_2			materials/textures/units/ground/spawner_morirot_damage_packed.dds
	set_texture_alias normal_2			materials/textures/units/ground/spawner_morirot_damage_normal.dds
	set_texture_alias emissive_2		materials/textures/units/ground/spawner_cosmic_morirot_damage_emissive.dds
}

material gui/menu/species/other/spawner_cosmic_morirot : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/species/other/spawner_cosmic_morirot.dds
}

material gui/hud/other/spawner_cosmic_morirot_icon : gui/unsharp
{
	set_texture_alias diffuse materials/textures/gui/hud/other/spawner_cosmic_morirot_icon.dds
}

