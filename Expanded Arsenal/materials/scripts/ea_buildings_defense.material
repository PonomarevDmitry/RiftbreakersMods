import * from "materials/scripts/pbr.material"
import * from "materials/scripts/buildings_base.material"

//----------------------ADVANCED ENERGY WALLS START----------------------\\

// wall_kinetic

material buildings/defense/wall_energy_base_kinetic
{
    
    render_group 61

    technique
    {
		pass
		{
			depth_write off
			scene_blend alpha_blend

			vertex_program_ref unlit_emissive_fresnel_fog_world_vp
            {
            }

			fragment_program_ref wall_energy_inverted_fp
			{
				param_named cEmissiveColor  float4 9 8 0 1
				param_named cFresnelColor 	float4 6 5 0 1
				param_named cFresnelBias    float 0.05
				param_named cFresnelScale   float 1
				param_named cFresnelPower   float 2
			}

            texture_unit diffuse
            {
                texture 		materials/textures/buildings/defense/wall_energy_lvl_1_kinetic_albedo.dds
                filtering 		anisotropic
            }

            texture_unit emissive
            {
                texture 		materials/textures/noise_tiled.dds
                filtering 		anisotropic
            }

            texture_unit gradient
            {
                anim_texture	materials/textures/gradient/cloudy_gradient.dds 174 3
				filtering 		bilinear
				tex_address_mode wrap
            }
        }
    }
}

material buildings/defense/wall_energy_lvl_1_kinetic : buildings/defense/wall_energy_base_kinetic
{
    set_texture_alias diffuse			materials/textures/buildings/defense/wall_energy_lvl_1_kinetic_albedo.dds
    set_texture_alias emissive			materials/textures/buildings/defense/wall_energy_lvl_1_energy_emissive.dds
}

