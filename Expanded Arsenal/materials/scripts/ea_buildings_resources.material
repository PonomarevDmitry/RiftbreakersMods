// crystal_dna_laboratory textures

material buildings/resources/crystal_dna_laboratory : pbr/reflections_alpha_test
{
	set_texture_alias albedo			materials/textures/buildings/resources/crystal_dna_laboratory_albedo.dds
	set_texture_alias normal			materials/textures/buildings/resources/crystal_dna_laboratory_normal.dds
	set_texture_alias packed			materials/textures/buildings/resources/crystal_dna_laboratory_packed.dds
	set_texture_alias emissive			materials/textures/buildings/resources/crystal_dna_laboratory_emissive.dds
	set_texture_alias reflection     	materials/textures/skybox/skybox_reflection_detailed_blue.dds
	set_texture_alias gradient			materials/textures/gradient/selector_gradient.dds 190 8

    glow 5
	reflection 0.2 6
}

material buildings/resources/crystal_dna_laboratory_dissolve : pbr/height_dissolve
{
	set_texture_alias albedo			materials/textures/buildings/resources/crystal_dna_laboratory_albedo.dds
	set_texture_alias normal			materials/textures/buildings/resources/crystal_dna_laboratory_normal.dds
	set_texture_alias packed			materials/textures/buildings/resources/crystal_dna_laboratory_packed.dds
	set_texture_alias emissive			materials/textures/buildings/resources/crystal_dna_laboratory_emissive.dds
	set_texture_alias reflection     	materials/textures/skybox/skybox_reflection_detailed_blue.dds
	set_texture_alias gradient			materials/textures/gradient/selector_gradient.dds 190 8

    glow 5
	reflection 0.2 6
}

material buildings/resources/crystal_dna_laboratory_screen : pbr/default
{
	set_texture_alias albedo			materials/textures/buildings/main/rift_station_base_screen_albedo.dds
	set_texture_alias packed			materials/textures/buildings/main/rift_station_base_screen_packed.dds
	set_texture_alias reflection     	materials/textures/skybox/skybox_reflection_detailed_blue.dds

	glow 2
	reflection 0.2 6

	technique
	{
		pass
		{
		    texture_unit emissive
		    {
		        anim_texture    materials/textures/buildings/resources/crystal_dna_laboratory_screen_emissive.dds 9 5
				filtering anisotropic
				max_anisotropy 16
		    }
		}
    }
}

material buildings/resources/crystal_dna_laboratory_screen_dissolve : pbr/height_dissolve
{
	set_texture_alias albedo			materials/textures/buildings/main/rift_station_base_screen_albedo.dds
	set_texture_alias packed			materials/textures/buildings/main/rift_station_base_screen_packed.dds
	set_texture_alias reflection     	materials/textures/skybox/skybox_reflection_detailed_blue.dds

	glow 2
	reflection 0.2 6

	technique
	{
		pass
		{
		    texture_unit emissive
		    {
		        anim_texture    materials/textures/buildings/main/rift_station_base_screen_emissive.dds 6 2
				filtering anisotropic
				max_anisotropy 16
		    }
		}
    }
}

