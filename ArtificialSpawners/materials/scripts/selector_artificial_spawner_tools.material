import * from "materials/scripts/selector.material"

material selector/marker_artificial_spawner_slots_picker_tool : selector/marker_base
{
	set_texture_alias diffuse			materials/textures/selector/marker_artificial_spawner_slots_picker_tool.dds
	set_texture_alias emissive			materials/textures/selector/marker_artificial_spawner_slots_picker_tool_emissive.dds

	technique
	{
		pass
		{
			depth_check			off
			depth_write			off
		}
	}
}

