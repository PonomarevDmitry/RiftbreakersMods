import * from "materials/scripts/decal.material"

material decals/range_circle_shield_red : decal/default
{
	set_texture_alias albedo			materials/textures/selector/range_circle_shield_red.dds
	set_texture_alias emissive			materials/textures/selector/range_circle_shield_red.dds
	glow 1
}

