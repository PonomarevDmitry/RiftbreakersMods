import * from "materials/scripts/selector.material"

material selector/marker_lamp_tool_sell : selector/marker_base
{
	set_texture_alias diffuse			materials/textures/selector/marker_lamp_tool_sell.dds
	set_texture_alias emissive			materials/textures/selector/marker_lamp_tool_sell_emissive.dds

	technique
	{
		pass
		{
			depth_check			off
			depth_write			off
		}
	}
}





material selector/marker_lamp_tool_sell_place_ruins : selector/marker_base
{
	set_texture_alias diffuse			materials/textures/selector/marker_lamp_tool_sell_place_ruins.dds
	set_texture_alias emissive			materials/textures/selector/marker_lamp_tool_sell_place_ruins_emissive.dds

	technique
	{
		pass
		{
			depth_check			off
			depth_write			off
		}
	}
}