	set_texture_alias emissive			materials/textures/buildings/resources/cryogenic_refinery_emissive.dds
}

material gui/hud/building_icons/cryogenic_refinery : gui/default
{
	set_texture_alias diffuse materials/textures/gui/hud/building_icons/cryogenic_refinery.dds
}

