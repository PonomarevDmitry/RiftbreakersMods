//buildings/resources/cryogenic_refinery build icons

material buildings/resources/cryogenic_refinery : buildings/destructible
{
	set_texture_alias albedo			materials/textures/buildings/resources/cryogenic_refinery_albedo.dds
	set_texture_alias normal			materials/textures/buildings/resources/supercoolant_refinery_normal.dds
	set_texture_alias packed			materials/textures/buildings/resources/supercoolant_refinery_packed.dds
	set_texture_alias emissive			materials/textures/buildings/resources/cryogenic_refinery_emissive.dds
}

