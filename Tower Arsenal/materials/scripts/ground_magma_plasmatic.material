import * from "materials/scripts/pbr.material"

material ground/plasmatic/plasmatic_01 : pbr/default
{
	terrain_type 	 "rocks"

	set_texture_alias normal 	materials/textures/ground/magma/lava_01_normal.dds
	set_texture_alias albedo 	materials/textures/ground/magma/lava_01_albedo.dds
	set_texture_alias packed 	materials/textures/ground/magma/lava_01_packed.dds
	set_texture_alias emissive 	materials/textures/ground/plasmatic/plasmatic_01_emissive.dds
}

