import * from "materials/scripts/units_base.material"
import * from "materials/scripts/gui.material"

material units/ground/cosmic_stregaros_boss : 		units/ground/black_blood
{
	set_texture_alias albedo			materials/textures/units/ground/cosmic_stregaros_boss_albedo.dds
	set_texture_alias normal			materials/textures/units/ground/stregaros_boss_acid_normal.dds
	set_texture_alias packed			materials/textures/units/ground/stregaros_boss_acid_packed.dds
	set_texture_alias emissive			materials/textures/units/ground/cosmic_stregaros_boss_emissive.dds
	//set_texture_alias gradient			materials/textures/gradient/cloudy_gradient.dds 168 5

	glow 10
}

material gui/menu/species/fauna/cosmic_stregaros_boss : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/species/fauna/cosmic_stregaros_boss.dds
}

material gui/hud/fauna/cosmic_stregaros_boss_icon : gui/unsharp
{
	set_texture_alias diffuse materials/textures/gui/hud/fauna/cosmic_stregaros_boss_icon.dds
}




material units/ground/cosmic_mushbit : 		units/ground/black_blood
{
	set_texture_alias albedo			materials/textures/units/ground/cosmic_mushbit_albedo.dds
	set_texture_alias normal			materials/textures/units/ground/mushbit_ultra_normal.dds
	set_texture_alias packed			materials/textures/units/ground/mushbit_ultra_packed.dds
	set_texture_alias emissive			materials/textures/units/ground/cosmic_mushbit_emissive.dds
	set_texture_alias gradient			materials/textures/gradient/cloudy_gradient.dds 168 5

	glow 15
}

material gui/menu/species/fauna/cosmic_mushbit : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/species/fauna/cosmic_mushbit.dds
}

material gui/hud/fauna/cosmic_mushbit_icon : gui/unsharp
{
	set_texture_alias diffuse materials/textures/gui/hud/fauna/cosmic_mushbit_icon.dds
}
