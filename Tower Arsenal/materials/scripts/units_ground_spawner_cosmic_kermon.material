import * from "materials/scripts/units_base.material"
import * from "materials/scripts/gui.material"
import * from "materials/scripts/pbr.material"

material units/ground/spawner_cosmic_base : 		pbr/vegetation
{
	set_texture_alias albedo			materials/textures/units/ground/spawner_cosmic_base_albedo.dds
	set_texture_alias normal			materials/textures/units/ground/spawrot_normal.dds
	set_texture_alias packed			materials/textures/units/ground/spawrot_packed.dds
	set_texture_alias emissive			materials/textures/units/ground/spawner_cosmic_base_emissive.dds
	set_texture_alias gradient			materials/textures/gradient/cloudy_gradient.dds 174 5

	glow 20
}

material gui/menu/species/other/spawner_cosmic_base : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/species/other/spawner_cosmic_base.dds
}

material gui/hud/other/spawner_cosmic_base_icon : gui/unsharp
{
	set_texture_alias diffuse materials/textures/gui/hud/other/spawner_cosmic_base_icon.dds
}

