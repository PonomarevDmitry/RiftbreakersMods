import * from "materials/scripts/pbr.material"

material props/special/cosmic_mushroom : pbr/default
{
	set_texture_alias albedo			materials/textures/props/special/underground_mushroom/cosmic_mushroom_albedo.dds
	set_texture_alias normal			materials/textures/props/special/underground_mushroom/underground_mushroom_medium_01_normal.dds
	set_texture_alias packed			materials/textures/props/special/underground_mushroom/underground_mushroom_medium_01_packed.dds
	set_texture_alias emissive			materials/textures/props/special/underground_mushroom/cosmic_mushroom_emissive.dds
	set_texture_alias gradient			materials/textures/gradient/cloudy_gradient.dds 168 5

	glow 5
}

