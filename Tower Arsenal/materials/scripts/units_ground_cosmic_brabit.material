import * from "materials/scripts/units_base.material"
import * from "materials/scripts/gui.material"

material units/ground/cosmic_oculus : 		units/ground/black_blood
{
	set_texture_alias albedo			materials/textures/units/ground/cosmic_oculus_albedo.dds
	set_texture_alias emissive			materials/textures/units/ground/cosmic_oculus_emissive.dds
	set_texture_alias gradient			materials/textures/gradient/cloudy_gradient.dds 168 5

	glow 15
}

material gui/menu/species/fauna/cosmic_oculus : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/species/fauna/cosmic_oculus.dds
}

material gui/hud/fauna/cosmic_oculus_icon : gui/unsharp
{
	set_texture_alias diffuse materials/textures/gui/hud/fauna/cosmic_oculus_icon.dds
}

