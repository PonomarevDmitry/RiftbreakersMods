import * from "materials/scripts/pbr.material"

material props/crystals/entvined_01_set_skin3 : pbr/reflections
{
	set_texture_alias albedo materials/textures/props/crystals/entvined_01_set_skin3_albedo.dds
	set_texture_alias normal materials/textures/props/crystals/entvined_01_set_skin3_normal.dds
	set_texture_alias packed materials/textures/props/crystals/entvined_01_set_skin3_packed.dds
	set_texture_alias emissive materials/textures/props/crystals/entvined_01_set_skin3_emissive.dds
	set_texture_alias reflection materials/textures/skybox/skybox_reflection_colorfull_01.dds
	
    glow 2
    reflection 1 30
}

