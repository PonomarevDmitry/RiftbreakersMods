import * from "materials/scripts/ground_resources.material"

material ground/resources/cosmonite : ground/resources/default
{
	set_texture_alias normal materials/textures/ground/resources/uranium_normal.dds
	set_texture_alias albedo materials/textures/ground/resources/cosmonite_albedo.dds
	set_texture_alias packed materials/textures/ground/resources/uranium_packed.dds
	set_texture_alias emissive materials/textures/ground/resources/cosmonite_emissive.dds
	
	glow 2
}

