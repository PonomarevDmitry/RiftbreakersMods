import * from "materials/scripts/units_base.material"
import * from "materials/scripts/gui.material"

material units/ground/cosmic_baxmoth_boss : 		units/ground/black_blood
{
	set_texture_alias albedo			materials/textures/units/ground/cosmic_baxmoth_boss_albedo.dds
	set_texture_alias normal			materials/textures/units/ground/baxmoth_ultra_normal.dds
	set_texture_alias packed			materials/textures/units/ground/baxmoth_ultra_packed.dds
	set_texture_alias emissive			materials/textures/units/ground/cosmic_baxmoth_boss_emissive.dds
	set_texture_alias gradient			materials/textures/gradient/cloudy_gradient.dds 168 5

	glow 15
}

material gui/menu/species/fauna/cosmic_baxmoth_boss : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/species/fauna/cosmic_baxmoth_boss.dds
}

material gui/hud/fauna/cosmic_baxmoth_boss_icon : gui/unsharp
{
	set_texture_alias diffuse materials/textures/gui/hud/fauna/cosmic_baxmoth_boss_icon.dds
}

