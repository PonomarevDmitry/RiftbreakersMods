import * from "materials/scripts/units_base.material"
import * from "materials/scripts/gui.material"

{
material units/air/baxmoth_drone_cosmic : 	units/ground/white_blood
{
	set_texture_alias albedo			materials/textures/units/air/baxmoth_drone_cosmic_albedo.dds
	set_texture_alias normal			materials/textures/units/air/baxmoth_drone_normal.dds
	set_texture_alias packed			materials/textures/units/air/baxmoth_drone_packed.dds
	set_texture_alias emissive			materials/textures/units/air/baxmoth_drone_cosmic_emissive.dds
	set_texture_alias gradient			materials/textures/gradient/cloudy_gradient.dds 168 5

	glow 15
}

material units/ground/cosmic_yeast_root_origin_set : pbr/vegetation_deform_no_alpha
{
	set_texture_alias albedo 		materials/textures/units/ground/cosmic_yeast_root_origin_set_albedo.dds
	set_texture_alias normal 		materials/textures/units/ground/creeper_root_origin_set_normal.dds
