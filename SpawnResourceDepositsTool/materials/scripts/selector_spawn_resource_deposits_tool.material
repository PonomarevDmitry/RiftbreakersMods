import * from "materials/scripts/selector.material"

material selector/marker_store_resources_tool : selector/marker_base
{
	set_texture_alias diffuse			materials/textures/selector/marker_store_resources_tool.dds
	set_texture_alias emissive			materials/textures/selector/marker_store_resources_tool_emissive.dds

	technique
	{
		pass
		{
			depth_check			off
			depth_write			off
		}
	}
}



