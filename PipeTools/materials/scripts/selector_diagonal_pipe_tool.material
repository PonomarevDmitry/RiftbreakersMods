import * from "materials/scripts/selector.material"

material selector/marker_diagonal_pipe_lines_1 : selector/marker_base
{
	set_texture_alias diffuse			materials/textures/selector/marker_diagonal_pipe_lines_1.dds
	set_texture_alias emissive			materials/textures/selector/marker_diagonal_pipe_lines_1_emissive.dds

	technique
	{
		pass
		{
			depth_check			off
			depth_write			off
		}
	}
}

