import * from "materials/scripts/selector.material"

material selector/marker_loot_collecting_tool : selector/marker_base
{
	set_texture_alias diffuse			materials/textures/selector/marker_loot_collecting_tool.dds
	set_texture_alias emissive			materials/textures/selector/marker_loot_collecting_tool_emissive.dds

	technique
	{
		pass
		{
			depth_check			off
			depth_write			off
		}
	}
}





material selector/marker_loot_collecting_all_map_tool : selector/marker_base
{
	set_texture_alias diffuse			materials/textures/selector/marker_loot_collecting_all_map_tool.dds
	set_texture_alias emissive			materials/textures/selector/marker_loot_collecting_all_map_tool_emissive.dds

	technique
	{
		pass
		{
			depth_check			off
			depth_write			off
		}
	}
}