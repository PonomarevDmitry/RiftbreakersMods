import * from "materials/scripts/pbr.material"

material resources/resource_liquid
{
	render_group 50
	feature "gbuffer|shadow"

	technique
	{
		pass
		{
			depth_write on
			depth_check on

			glow 1

			vertex_program_ref deferred_resource_liquid_vp
			{
			    param_named      cTilingFactor  float 2
			}

			fragment_program_ref deferred_resource_liquid_fp
			{

			}

			texture_unit albedo_1
			{
				texture materials/textures/error.dds
				filtering anisotropic
				max_anisotropy 16
			}
			texture_unit normal_1
			{
				texture materials/textures/normalmap_flat_bc5.dds
				filtering anisotropic
				max_anisotropy 16
			}
			texture_unit packed_1
			{
				texture materials/textures/color_white.dds
				filtering anisotropic
				max_anisotropy 16
			}
			texture_unit emissive_1
			{
				texture materials/textures/color_black.dds
				filtering anisotropic
				max_anisotropy 16
			}
			texture_unit albedo_2
			{
				texture materials/textures/error.dds
				filtering anisotropic
				max_anisotropy 16
			}
			texture_unit normal_2
			{
				texture materials/textures/normalmap_flat_bc5.dds
				filtering anisotropic
				max_anisotropy 16
			}
			texture_unit packed_2
			{
				texture materials/textures/color_white.dds
				filtering anisotropic
				max_anisotropy 16
			}
			texture_unit emissive_2
			{
				texture materials/textures/color_black.dds
				filtering anisotropic
				max_anisotropy 16
			}
			texture_unit noise
			{
				texture materials/textures/noise_tiled.dds
				filtering anisotropic
				max_anisotropy 16
			}
			texture_unit gradient
			{
				texture materials/textures/noise_tiled.dds
				filtering bilinear
			}
			texture_unit flowmap
			{
				texture materials/textures/flowmap/sand_01_flowmap.dds
				filtering anisotropic
				max_anisotropy 16
			}
		}
	}
}

