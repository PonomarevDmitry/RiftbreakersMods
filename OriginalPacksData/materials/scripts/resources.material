material resources/resource_iron : resources/resource_vein
{
	set_texture_alias albedo			materials/textures/resources/resource_iron_albedo.dds
	set_texture_alias normal			materials/textures/resources/resource_iron_normal.dds
	set_texture_alias packed			materials/textures/resources/resource_iron_packed.dds
	set_texture_alias gradient			materials/textures/gradient/selector_gradient.dds 190 8

	glow 3

	technique
	{
		pass
		{
			fragment_program_ref deferred_resource_vein_fp
			{
        		param_named      	cGlowFactor         float  1
				param_named 	 	cFresnelColor 		float4 3 1 0 1
				param_named 	 	cFresnelScale   	float 1
				param_named 	 	cFresnelPower   	float 3
			}
		}
	}
}

