import * from "materials/scripts/pbr.material"

material props/rocks/anoryx_red/anoryx_red_big_01 : pbr/reflections_subsurface
{
    set_texture_alias albedo           materials/textures/props/rocks/anoryx_red/anoryx_red_big_01_albedo.dds
    set_texture_alias normal           materials/textures/props/rocks/anoryx_red/anoryx_red_big_01_normal.dds
    set_texture_alias packed           materials/textures/props/rocks/anoryx_red/anoryx_red_big_01_packed.dds
    set_texture_alias emissive         materials/textures/props/rocks/anoryx_red/anoryx_red_big_01_emissive.dds
    set_texture_alias subsurface       materials/textures/props/rocks/anoryx_red/anoryx_red_big_01_subsurface.dds
    //set_texture_alias gradient         materials/textures/gradient/waves.dds 180 4
    set_texture_alias reflection       materials/textures/skybox/skybox_reflection.dds
   
    glow 1
    reflection 1 3  

    
}

