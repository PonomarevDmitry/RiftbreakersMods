import * from "materials/scripts/pbr.material"

material ground/sand/wet_sand : pbr/default
{
	terrain_type 	 "sand"

	set_texture_alias normal materials/textures/ground/sand/wet_sand_normal.dds
	set_texture_alias albedo materials/textures/ground/sand/wet_sand_albedo.dds
	set_texture_alias packed materials/textures/ground/sand/wet_sand_packed.dds
}

