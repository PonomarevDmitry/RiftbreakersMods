import * from "materials/scripts/units_base.material"

material units/air/vathamite: pbr/vegetation_deform
{
	set_texture_alias albedo materials/textures/units/air/vathamite_albedo.dds
	set_texture_alias normal materials/textures/units/air/vathamite_normal.dds
	set_texture_alias packed materials/textures/units/air/vathamite_packed.dds
	set_texture_alias emissive materials/textures/units/air/vathamite_emissive.dds

 	glow 1
	deform .3 1.5
	subsurface .5 .7 .7 .7
}

