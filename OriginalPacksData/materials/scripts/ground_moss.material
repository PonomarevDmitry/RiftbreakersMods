import * from "materials/scripts/pbr.material"

material ground/moss/moss : pbr/default
{
	terrain_type 	 "foliage"

	set_texture_alias normal materials/textures/ground/moss/moss_normal.dds
	set_texture_alias albedo materials/textures/ground/moss/moss_albedo.dds
	set_texture_alias packed materials/textures/ground/moss/moss_packed.dds
}

