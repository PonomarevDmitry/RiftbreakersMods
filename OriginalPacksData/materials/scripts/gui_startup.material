import * from "materials/scripts/gui.material"

material gui/loading/loading_icon_loop : gui/default
{	
	set_texture_alias diffuse			materials/textures/gui/loading/loading_icon.dds 59 3
}

material gui/loading/background_earth_hires : gui/default
{
	set_texture_alias diffuse			materials/textures/gui/loading/background_earth_hires.dds
}

material gui/loading/black_bg : gui/default
{
	set_texture_alias diffuse			materials/textures/gui/loading/black_bg.dds
}

material gui/loading/text_bg_left : gui/default
{
	set_texture_alias diffuse			materials/textures/gui/loading/text_bg_left.dds
}

material gui/menu/buttons/customize_controls_neutral_selected : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/buttons/customize_controls_neutral_selected.dds
}

material gui/menu/buttons/customize_controls_neutral : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/buttons/customize_controls_neutral.dds
}

material gui/menu/pop_up_top : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/pop_up_top.dds
}

material gui/menu/pop_up_center : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/pop_up_center.dds
	technique
	{
		pass
		{
			texture_unit			diffuse
			{
				tex_address_mode	wrap
			}
		}
	}
}

material gui/menu/pop_up_bottom : gui/default
{
	set_texture_alias diffuse materials/textures/gui/menu/pop_up_bottom.dds
}
