import * from "materials/scripts/pbr.material"

material props/grass/black_set : pbr/vegetation
{
	set_texture_alias albedo materials/textures/props/grass/black_set_albedo.dds
	set_texture_alias normal materials/textures/props/grass/black_set_normal.dds
	set_texture_alias packed materials/textures/props/grass/black_set_packed.dds

	subsurface  0.6 0.8 0 0
}

