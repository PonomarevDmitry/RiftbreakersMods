import * from "materials/scripts/pbr.material"

material props/vines/liana_set : pbr/vegetation_twosided

{
	set_texture_alias albedo		materials/textures/props/vines/liana_set_albedo.dds
	set_texture_alias normal			materials/textures/props/vines/liana_set_normal.dds
	set_texture_alias packed			materials/textures/props/vines/liana_set_packed.dds

	subsurface 0.3 0.3 0.5 0.7

}

material props/vines/liana_set_skin1 : pbr/vegetation_twosided

{
	set_texture_alias albedo		materials/textures/props/vines/liana_set_skin1_albedo.dds
	set_texture_alias normal			materials/textures/props/vines/liana_set_skin1_normal.dds
	set_texture_alias packed			materials/textures/props/vines/liana_set_skin1_packed.dds
	set_texture_alias emissive			materials/textures/props/vines/liana_set_skin1_emissive.dds

	subsurface 0.3 0.3 0.5 0.7
	glow 4

}

