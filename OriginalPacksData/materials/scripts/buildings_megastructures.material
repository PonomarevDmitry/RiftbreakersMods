material buildings/megastructures/glass : pbr/glass
{
	set_texture_alias normal materials/textures/buildings/megastructures/glass_normal.dds
	set_texture_alias albedo materials/textures/buildings/megastructures/glass_albedo.dds
	set_texture_alias packed materials/textures/buildings/megastructures/glass_packed.dds

	technique
	{
		pass
		{
			fragment_program_ref forward_glass_fp
			{
        param_named         cEdgeColor          float4         1 1 1 1
			}
		}
	}
}

material buildings/megastructures/glass_skinned : pbr/glass_skinned
{
