material buildings/megastructures/megastructures_plasma : pbr/default
{
		set_texture_alias albedo			materials/textures/buildings/megastructures/megastructures_plasma_albedo.dds
		set_texture_alias emissive		materials/textures/buildings/megastructures/megastructures_plasma_emissive.dds

		glow 6.5

    technique
    {
        pass
        {
						vertex_program_ref deferred_gbuffer_anim_vp
						{
				     	param_named      cTilingSpeed   float2 -0.4 0.08
						}
        }
    }
}

material buildings/megastructures/megastructures_plasma_dissolve : pbr/height_dissolve
{
		set_texture_alias albedo			materials/textures/buildings/megastructures/megastructures_plasma_albedo.dds
		set_texture_alias emissive		materials/textures/buildings/megastructures/megastructures_plasma_emissive.dds

		glow 6.5
}

material buildings/megastructures/screen : buildings/destructible
{

	set_texture_alias albedo			materials/textures/buildings/main/rift_station_base_screen_albedo.dds
	set_texture_alias packed			materials/textures/buildings/main/rift_station_base_screen_packed.dds

	glow 2.5

	technique
	{
		pass
			{
			    texture_unit emissive
			    {
			        anim_texture    materials/textures/buildings/megastructures/quantum_cortex_labolatory_screen.dds 37 1.2
					filtering anisotropic
					max_anisotropy 16
			    }
			}
    }

}

material buildings/megastructures/screen_dissolve : pbr/height_dissolve
{

	set_texture_alias albedo			materials/textures/buildings/main/rift_station_base_screen_albedo.dds
	set_texture_alias packed			materials/textures/buildings/main/rift_station_base_screen_packed.dds

	glow 2.5
}

