material props/special/loot_containers/loot_container_small_bulb_membrane
{
    render_group 61

    technique
    {
		pass
		{
			depth_write off
			scene_blend alpha_blend
			cull_hardware clockwise

			vertex_program_ref forward_object_texture_fresnel_fog_world_vp
            {
            }

			fragment_program_ref wall_energy_inverted_fp
			{
				param_named cEmissiveColor  float4 0 2 1 1
				param_named cFresnelColor 	float4 5 2 1.5 1
				param_named cFresnelBias    float 0.04
				param_named cFresnelScale   float 2
				param_named cFresnelPower   float 4
			}

            texture_unit diffuse
            {
                texture 		materials/textures/props/special/loot_containers/loot_container_small_bulb_membrane_albedo.dds
                filtering 		anisotropic
            }

            texture_unit emissive
            {
                texture 		materials/textures/noise_tiled.dds
                filtering 		anisotropic
            }

            texture_unit gradient
            {
                anim_texture	materials/textures/gradient/cloudy_gradient.dds 174 2
								filtering 		bilinear
								tex_address_mode wrap
            }

            texture_unit volumetric_fog
			{
				texture 			materials/textures/black_volume.dds
				texture_type 		3d
				content_type 		shared volumetric_fog
				tex_address_mode 	clamp
				filtering 			bilinear
			}
        }
    }
}

