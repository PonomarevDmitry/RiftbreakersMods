material props/special/interactive/poogret_poo_fresnel
{
    render_group 61

    technique
    {
        pass
        {
            depth_write off
            scene_blend alpha_blend
            depth_bias -10 -1

            vertex_program_ref unlit_fresnel_vp
			{
			}

			fragment_program_ref unlit_fresnel_flare_fp
			{
				param_named cFresnelBias  	float -0.1
				param_named cFresnelScale 	float 1.2
				param_named cFresnelPower 	float 2

				//param_named cBlendColor1 	float4 2 0 1 0
				//param_named cBlendColor2 	float4 0 0.2 2 0.5
				param_named cBlendColor1 	float4 0 0.7 2 0
				param_named cBlendColor2 	float4 2 0 1 0.5

				param_named cFlarePower 	float 0.2
				param_named cFlareSpeed   	float 6
			}
        }
    }
}

