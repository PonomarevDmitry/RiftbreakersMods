	set_texture_alias albedo			materials/textures/props/special/loot_containers/loot_container_desert_small_standard_albedo.dds
	set_texture_alias normal			materials/textures/props/special/loot_containers/loot_container_desert_small_standard_normal.dds
	set_texture_alias packed			materials/textures/props/special/loot_containers/loot_container_desert_small_standard_packed.dds
	set_texture_alias emissive			materials/textures/props/special/loot_containers/loot_container_desert_small_standard_emissive.dds
	set_texture_alias gradient			materials/textures/gradient/pulsing01.dds 60 5

	glow 4
	deform 0.08 0.7
	subsurface 0.1 0.1 0.1 0.1
}

