import * from "materials/scripts/pbr.material"

material props/rocks/hex_crystal/hex_crystal_big_01 : pbr/reflections
{
    set_texture_alias albedo           materials/textures/props/rocks/hex_crystal/hex_crystal_big_01_albedo.dds
    set_texture_alias normal           materials/textures/props/rocks/hex_crystal/hex_crystal_big_01_normal.dds
    set_texture_alias packed           materials/textures/props/rocks/hex_crystal/hex_crystal_big_01_packed.dds
    set_texture_alias emissive         materials/textures/props/rocks/hex_crystal/hex_crystal_big_01_emissive.dds
    set_texture_alias reflection       materials/textures/skybox/skybox_reflection_clouds.dds

    glow 1.6
    reflection 0.6 4
}

