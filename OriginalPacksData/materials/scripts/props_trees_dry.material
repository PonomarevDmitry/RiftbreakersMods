import * from "materials/scripts/pbr.material"

material props/trees_dry/twisted_big_set : pbr/vegetation_tree
{
	set_texture_alias albedo materials/textures/props/trees_dry/twisted_big_set_albedo.dds
	set_texture_alias normal materials/textures/props/trees_dry/twisted_big_set_normal.dds
	set_texture_alias packed materials/textures/props/trees_dry/twisted_big_set_packed.dds
	
	subsurface 0 0 0 0
}

