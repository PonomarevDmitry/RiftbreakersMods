import * from "materials/scripts/pbr.material"

material props/crystals/pure_set : pbr/reflections
{
	set_texture_alias albedo			materials/textures/props/crystals/pure_set_albedo.dds
	set_texture_alias normal			materials/textures/props/crystals/pure_set_normal.dds
	set_texture_alias packed			materials/textures/props/crystals/pure_set_packed.dds
	set_texture_alias reflection        materials/textures/skybox/skybox_reflection_colorfull.dds

	technique
	{
		pass
		{
			glow 0

			fragment_program_ref deferred_gbuffer_reflection_fp
			{
        		param_named      cReflectionParams   float2         0.5 0.3
			}

		}
	}
}

