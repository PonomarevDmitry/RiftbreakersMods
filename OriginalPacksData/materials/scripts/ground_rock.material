import * from "materials/scripts/pbr.material"

material ground/rock/small_rocks_01 : pbr/default
{
	terrain_type 	 "rocks"

	set_texture_alias normal materials/textures/ground/rock/small_rocks_01_normal.dds
	set_texture_alias albedo materials/textures/ground/rock/small_rocks_01_albedo.dds
	set_texture_alias packed materials/textures/ground/rock/small_rocks_01_packed.dds
}

