import * from "materials/scripts/pbr.material"

material props/bush/ray_flower_very_small : pbr/vegetation_twosided
{
	set_texture_alias albedo materials/textures/props/bush/ray_flower_very_small_albedo.dds
	set_texture_alias normal materials/textures/props/bush/ray_flower_very_small_normal.dds
	set_texture_alias packed materials/textures/props/bush/ray_flower_very_small_packed.dds
	set_texture_alias emissive materials/textures/props/bush/ray_flower_very_small_emissive.dds

	glow 4
	subsurface 0.5 0.7 0.7 0.7
}

