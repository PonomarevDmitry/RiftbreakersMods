	set_texture_alias packed			materials/textures/units/ground/canoptrix_crystal_alpha_packed.dds
	set_texture_alias emissive			materials/textures/units/ground/canoptrix_crystal_alpha_emissive.dds
	set_texture_alias reflection        materials/textures/skybox/skybox_reflection_colorfull.dds

