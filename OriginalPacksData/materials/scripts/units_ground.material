material units/ground/canoptrix_crystal : pbr/reflections
{
	set_texture_alias albedo			materials/textures/units/ground/canoptrix_crystal_albedo.dds
	set_texture_alias normal			materials/textures/units/ground/canoptrix_crystal_normal.dds
	set_texture_alias packed			materials/textures/units/ground/canoptrix_crystal_packed.dds
	set_texture_alias emissive			materials/textures/units/ground/canoptrix_crystal_emissive.dds
	set_texture_alias reflection        materials/textures/skybox/skybox_reflection_colorfull.dds

	technique
	{
		pass
		{
			glow 0.8

			fragment_program_ref deferred_gbuffer_reflection_fp
			{
        		param_named      cReflectionParams   float2         0.5 0.3
			}

		}
	}
}

material units/ground/canoptrix_crystal_alpha : pbr/reflections
