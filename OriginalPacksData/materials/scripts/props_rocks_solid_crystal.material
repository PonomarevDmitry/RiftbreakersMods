import * from "materials/scripts/pbr.material"


material props/rocks/solid_crystal/solid_crystal_big_01_03 : pbr/reflections
{
    set_texture_alias albedo           materials/textures/props/rocks/solid_crystal/solid_crystal_big_01_03_albedo.dds
    set_texture_alias normal           materials/textures/props/rocks/solid_crystal/solid_crystal_big_01_03_normal.dds
    set_texture_alias packed           materials/textures/props/rocks/solid_crystal/solid_crystal_big_01_03_packed.dds
    set_texture_alias emissive         materials/textures/props/rocks/solid_crystal/solid_crystal_big_01_03_emissive.dds
    set_texture_alias reflection       materials/textures/skybox/skybox_reflection.dds
    //set_texture_alias gradient            materials/textures/gradient/cloudy_gradient.dds 174 12

    glow 1
    reflection 1 2
}

