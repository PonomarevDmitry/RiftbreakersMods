import * from "materials/scripts/selector.material"

material selector/marker_gaps_count_0 : selector/marker_base
{
	set_texture_alias diffuse			materials/textures/selector/marker_gaps_count_0.dds
	set_texture_alias emissive			materials/textures/selector/marker_gaps_count_0_emissive.dds

	technique
	{
		pass
		{
			depth_check			off
			depth_write			off
		}
	}
}

